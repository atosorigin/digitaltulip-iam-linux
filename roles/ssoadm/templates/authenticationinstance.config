iplanet-am-auth-ldap-ssl-enabled=false
iplanet-am-auth-ldap-return-user-dn=true
iplanet-am-auth-ldap-base-dn=dc={{env}},dc=canopy-cloud,dc=com
iplanet-am-ldap-user-creation-attr-list=
iplanet-am-auth-ldap-server=ad.{{env}}.canopy-cloud.com:389
sunAMAuthADAuthLevel=0
openam-auth-ldap-heartbeat-interval=1
iplanet-am-auth-ldap-invalid-chars=*|(|)|&|!
iplanet-am-auth-ldap-user-naming-attribute=sAMAccountName
iplanet-am-auth-ldap-bind-passwd={{windows_ssh_pass}}
iplanet-am-auth-ldap-server2=
openam-auth-ldap-operation-timeout=0
iplanet-am-auth-ldap-search-scope=SUBTREE
iplanet-am-auth-ldap-search-filter=
iplanet-am-auth-ldap-user-search-attributes=sAMAccountName
iplanet-am-auth-ldap-bind-dn=CN=Administrator,CN=Users,DC={{env}},DC=canopy-cloud,DC=com
openam-auth-ldap-heartbeat-timeunit=MINUTES